<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tienda.a_shop">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".activity.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MyActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.AgregarProductoActivity"
            android:label="@string/title_activity_agregar_producto"></activity>
        <activity
            android:name=".activity.ListaItemsProductosActivity"
            android:label="@string/title_activity_carrito_de_venta"></activity>
        <activity
            android:name=".activity.ListaProductosActivity"
            android:label="@string/title_activity_list"></activity>
        <activity
            android:name=".activity.EditarProductoActivity"
            android:label="Editar Producto"></activity>
        <activity
            android:name=".activity.HistorialVentasActivity"
            android:label="@string/title_activity_historial_ventas"></activity>
        <activity
            android:name=".activity.AgregarItemGastoActivity"
            android:label="@string/title_activity_agregar_item_gasto"></activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" /> -->
    </application>

</manifest>